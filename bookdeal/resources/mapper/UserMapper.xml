        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yanqin.bookdeal.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.yanqin.bookdeal.domain.User">
